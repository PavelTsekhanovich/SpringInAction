<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="saxophone" class="by.spring.action.springidol.Saxophone" autowire-candidate="false"/>

    <bean id="poem" class="by.spring.action.springidol.Sonnet29"/>

    <bean id="duke" class="by.spring.action.springidol.PoeticJuggler" autowire="constructor"/>

</beans>
